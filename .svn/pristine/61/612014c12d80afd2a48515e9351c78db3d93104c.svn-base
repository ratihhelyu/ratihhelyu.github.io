.kn-spin {
   animation: kn-spin 2s infinite linear;
}

@keyframes kn-spin {
   0% {
      transform: rotate(0deg);
   }

   100% {
      transform: rotate(359deg);
   }
}

.kn-animate-fading {
   animation: fading 10s infinite;
}

@keyframes fading {
   0% {
      opacity: 0;
   }

   50% {
      opacity: 1;
   }

   100% {
      opacity: 0;
   }
}

.kn-animate-opacity {
   animation: opac 0.8s;
}

@keyframes opac {
   from {
      opacity: 0;
   }

   to {
      opacity: 1;
   }
}

.kn-animate-top {
   position: relative;
   animation: animatetop 0.4s;
}

@keyframes animatetop {
   from {
      top: -300px;
      opacity: 0;
   }

   to {
      top: 0;
      opacity: 1;
   }
}

.kn-animate-left {
   position: relative;
   animation: animateleft 0.4s;
}

@keyframes animateleft {
   from {
      left: -300px;
      opacity: 0;
   }

   to {
      left: 0;
      opacity: 1;
   }
}

.kn-animate-right {
   position: relative;
   animation: animateright 0.4s;
}

@keyframes animateright {
   from {
      right: -300px;
      opacity: 0;
   }

   to {
      right: 0;
      opacity: 1;
   }
}

.kn-animate-bottom {
   position: relative;
   animation: animatebottom 0.4s;
}

@keyframes animatebottom {
   from {
      bottom: -300px;
      opacity: 0;
   }

   to {
      bottom: 0;
      opacity: 1;
   }
}

.kn-animate-zoom {
   animation: animatezoom 0.6s;
}

@keyframes animatezoom {
   from {
      transform: scale(0);
   }

   to {
      transform: scale(1);
   }
}

